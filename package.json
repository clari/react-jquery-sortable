{
  "name": "react-jquery-sortable",
  "version": "0.1.0-alpha.4",
  "description": "React wrapper for jQuery sortable",
  "repository": "clari/react-jquery-sortable",
  "license": "MIT",
  "main": "lib",
  "files": [
    "dist",
    "lib"
  ],
  "keywords": [
    "jquery",
    "jquery-ui",
    "react",
    "react-component",
    "sortable"
  ],
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.config.dev.js",
    "build-dist": "rm -rf dist && NODE_ENV=production webpack --config webpack.config.dist.js",
    "build-lib": "rm -rf lib && NODE_ENV=production babel src --out-dir lib --only index.js,Sortable.jsx",
    "prepublish": "npm run build-lib && npm run build-dist"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.4",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.20.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "expose-loader": "^0.7.1",
    "immutable": "^3.8.1",
    "lodash": "^4.17.2",
    "node-sass": "^4.1.0",
    "postcss-loader": "^1.2.1",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-immutable-proptypes": "^2.1.0",
    "sass-loader": "^4.1.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
